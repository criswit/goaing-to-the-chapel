{
  "name": "wedding-website-cdk",
  "version": "0.1.0",
  "bin": {
    "wedding-website-cdk": "bin/wedding-website-cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
