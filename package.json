    "format:check": "prettier --check \"lib/**/*.{ts,tsx,js,jsx,json,css,md}\" \"test/**/*.{ts,tsx,js,jsx,json,css,md}\" \"bin/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "aws-cdk": "2.1027.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.42.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.211.0",
    "constructs": "^10.0.0"
  }
